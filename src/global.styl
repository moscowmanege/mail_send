body
	margin 0
	padding 0
	background-color #e2e2e2

.center
	background-color #e2e2e2
	padding-bottom 15px

table
	background-color #ffffff
	margin 0
	padding 0
	padding-left 15px
	padding-right 15px
	font-family Arial
	font-size 12pt
	line-height 14pt
	sup
		font-size 9pt
		line-height 0
	a
		color black
		text-decoration none
		font-weight bold
	td
		padding-top 15px
		vertical-align top
	.line td
		padding-top 30px
		width 650px
		hr
			padding 0
			margin 0
	.image img
		width 100%
		display block
	.image_desc td
		padding-top 30px
		span
			display inline-block
			border-bottom 1px solid black

.header_block
	padding-top 15px
	width 650px
	td
		padding-top 0
	.logo
		width 120px
		img
			width 100%
	.h1
		font-size 16pt
		line-height 18pt
		font-weight bold
	.h2
		line-height 16pt


.content_block
	width 650px
	.title
		&.h1
			font-size 16pt
			line-height 18pt
		&.h2
			font-weight bold
			line-height 16pt
		&.head td.head
				font-weight bold
		&.desc td.desc
				font-weight bold
	.spacer
		height 20px
	.category
		&.head td.head span
			font-weight bold
		&.val td.val span
			font-weight bold
		&.addon td.addon
			font-weight bold
		.head
			width 305px
			span
				width 100%
				display inline-block
				vertical-align bottom
				border-bottom 1px solid black
		.val span
			white-space nowrap
			display inline-block
			vertical-align bottom
			border-bottom 1px solid black
		.addon
			vertical-align bottom
			padding-left 15px


.footer_block
	width 650px
	padding-bottom 15px
	td
		width 320px
		vertical-align bottom
	.intro
		font-weight bold